{
  "expo": {
    "name": "AestheticAI",
    "slug": "frontend",
    "owner": "nllynnn",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "aestheticai",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-image.png",
      "resizeMode": "contain",
      "backgroundColor": "#F0E7FF"
    },
    "ios": {
      "supportsTablet": true,
      "backgroundColor": "#F0E7FF"
    },
    "android": {
      "package": "com.pedragozanlynn.aestheticai",
      "icon": "./assets/icon.png",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icon.png",
      "backgroundColor": "#F0E7FF"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "378347b9-d92e-49b4-8e56-824914724190"
      }
    }
  }
}
